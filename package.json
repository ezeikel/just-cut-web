{
  "name": "justcutt",
  "version": "0.0.0",
  "private": true,
  "engines": {
    "node": ">= 9.2.0"
  },
  "scripts": {
    "start": "node ./start.js",
    "watch": "nodemon ./start.js --ignore public/",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run watch\" \"npm run client\"",
    "server": "concurrently \"npm run watch\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "cacheDirectories": [
    "node_modules",
    "client/node_modules"
  ],
  "browserslist": "last 3 versions",
  "dependencies": {
    "aws-sdk": "^2.216.1",
    "body-parser": "1.17.1",
    "connect-mongo": "1.3.2",
    "cookie-parser": "1.4.3",
    "dotenv": "4.0.0",
    "es6-promisify": "5.0.0",
    "express": "4.15.2",
    "express-graphql": "^0.6.12",
    "express-session": "1.15.1",
    "express-validator": "3.1.2",
    "faker": "4.1.0",
    "forever": "0.15.3",
    "glob": "7.1.1",
    "graphql": "^0.13.2",
    "html-to-text": "3.2.0",
    "jimp": "^0.2.28",
    "juice": "4.0.2",
    "moment": "2.19.3",
    "mongoose": "4.8.7",
    "mongoose-mongodb-errors": "0.0.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "node-fetch": "^1.7.3",
    "nodemailer": "3.1.5",
    "passport": "0.3.2",
    "passport-local": "1.0.0",
    "passport-local-mongoose": "4.0.0",
    "pug": "2.0.0-beta6",
    "serve-favicon": "~2.4.5",
    "slugs": "0.1.3",
    "uuid": "^3.2.1",
    "validator": "7.0.0"
  },
  "keywords": [
    "node",
    "heroku",
    "create-react-app",
    "react"
  ],
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "concurrently": "3.4.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "nodemon": "1.11.0"
  }
}
